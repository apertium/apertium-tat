#         ^өйдә/өй<n><loc>+и<cop><p3><sg>$^./.<sent>$
 REMOVE Attr IF
        (NOT -1* Nom) (2 EOS OR MARK) ;

#
 SELECT Advl IF
        (0 A) (0/1 Postadv)
        (NOT -1 Det) ;
## Әхмәт [0] тиз генә бер агачка таба йөгерә.

